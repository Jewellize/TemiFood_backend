{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "rewrites": [
    {
      "source": "/public/images/(.*)",
      "destination": "/public/images/$1"
    }
  ]
}


