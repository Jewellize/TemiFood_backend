{   
      "version": 2,   
      "builds": [
        { 
          "src": "public/*/",
          "use": "@vercel/static" 
        }
      ],   
      "routes": [     
        { 
          "src": "/image/(.)", 
          "dest": "/public/images/$1" 
        },     
        { 
          "src": "/(.)", 
          "dest": "/$1" 
        }   
      ] 
  }


