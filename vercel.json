{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
    }
    ],
    "routes": [
      
      {
        "src": "/images",
        "dest": "public/images"
      },
      {
        "src": "/(.+)",
        "dest": "public/$1"
      },
      {
        "src": "/api/(.)",
        "dest": "index.js"
      }

    ]
}


